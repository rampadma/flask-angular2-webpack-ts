{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "typings/main.d.ts",
    "typings/main",
    "dist"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {"rewriteTsConfig": false}
}